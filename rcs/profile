# Jien's profile file.
# Shell chore functions to get preferencable programs.
	if [ -f $HOME/.CODE ] ; then
		CODE="`cat $HOME/code`"
	else
		CODE="$HOME/code"
	fi
	export CODE
	export SHMODS="$CODE/modules/sh"
	. $SHMODS/chores/progpref.sh
	. $SHMODS/chores/expvar.sh
# Main varibles for my workflow. All the paths depends on this.
	expvar ETC "$HOME/etc"
	expvar GIT "$HOME/git"
	expvar SHARE "$HOME/share"
	expvar BIN "$HOME/bin"
	expvar TMPS "$HOME/tmps"
	expvar TMP "$HOME/tmp"
	expvar PATH "$PATH:$BIN"
# Lazy to type uname every fucking time.
	#export ARCH="'`uname -p`'"
	expvar ARCH "`uname -p`"
	expvar OPSYS "`uname -s`"
	expvar KERNEL_RELEASE "`uname -r`"
# Man.
	expvar MANPATH "$SHARE/man:$MANPATH"
	MANSECT="1:n:l:8:3:0:2:5:4:9:6:7"
	# My man sections which I'll maybe use later.
	MANSECT="$MANSECT:m"
	expvar MANSECT "$MANSECT"
# pkgsrc.
	expvar PKG_VERSIONS "$KERNEL_RELEASE"
	PKG_PATH="$PKG_PATH;http://cdn.NetBSD.org/pub/pkgsrc/packages/$OPSYS/$ARCH/$KERNEL_RELEASE/All"
	expvar PKG_PATH "$PKG_PATH"
# General.
	expvar KEYBOARD_REPEAT_RATE  60
	expvar KEYBOARD_REPEAT_DELAY 300
# XDG.
	expvar XDG_CONFIG_HOME "$HOME/.config"
# Locale.
	expvar LANG "C"
	expvar LC_CTYPE "ru_RU.UTF-8"
# Wayland.
	expvar WLC_REPEAT_RATE "$KEYBOARD_REPEAT_RATE"
	expvar WLC_REPEAT_DELAY "$KEYBOARD_REPEAT_DELAY"
# Xorg.
# Menu.
	expvar WMENU "`progpref dmenu-wl`"
	expvar XMENU "`progpref dmenu`"
	expvar TMENU "`progpref fzf`"
# Lock.
	expvar XLOCK "`progpref slock`"
	expvar WLOCK "`progpref swaylock`"
# Editor.
	expvar EDITOR "`progpref nvim vim vi nano ex`"
	expvar GIT_EDITOR "$EDITOR"
	expvar VISUAL "`progpref gvim notepadqq leafpad mousepad adie gedi`"
# Terminal.
	expvar WTERMINAL "`progpref termite`"
	expvar XTERMINAL "`progpref st termite`"
# Pager.
	expvar PAGER "`progpref most less more`"
	expvar LESS "-R"
	expvar MANPAGER "$PAGER"
	expvar HELPPAGER "$PAGER"
# Less colors
	expvar LESS_TERMCAP_md $'\E[01;31m'
	expvar LESS_TERMCAP_me $'\E[0m'
	# Underlined
	expvar LESS_TERMCAP_us $'\E[01;32m'
	expvar LESS_TERMCAP_ue $'\E[0m'
	# Service info
	expvar LESS_TERMCAP_so $'\E[01;33m'
	expvar LESS_TERMCAP_se $'\E[0m'
	# Blinking color
	expvar LESS_TERMCAP_mb $'\E[1;34m'
	expvar LESS_TERMCAP_me $'\E[0m'
# Browser.
	expvar XBROWSER "`progpref waterfox firefox chromium opera midori epiphany jsurf luakit vimb surf`"
	expvar WBROWSER "`progpref waterfox firefox midori epiphany`"
	expvar TBROWSER "`progpref links2 links elinks w3m`"
# Sound.
	expvar JACKSNDDRIVE "alsa"
	expvar JACKDPSRATE "48000"
	expvar JACKSNDDEVICE "hw:0"
	expvar JACKNPERIODS "2"
	expvar JACKDPSPERIOD "64"
# GCC.
	expvar C_INCLUDE_PATH "$S/modules/c"
# Perl.
	expvar PERL5LIB "$S/modules/pl/5"
# Python.
	expvar PYTHONPATH "$CODE/modules/py"
# Nethack.
	expvar NETHACKOPTIONS "boulder:0, color, autodig"
# Irrlicht.
	expvar PATHTO_IRRLICHT "$GIT/irrlicht"
# NTK.
	expvar PATHTO_NTK="$GIT/ntk"
