# Jien's profile file.
# Shell chore functions to get preferencable programs export variable more flexible way.
	if [ -f $HOME/.CODE ] ; then
		CODE="`cat $HOME/.CODE`"
	else
		CODE="$HOME/code"
	fi
	export CODE
	export SHMODS="$CODE/modules/sh"
	. $SHMODS/chores/progpref.sh
	. $SHMODS/chores/expvar.sh
get_inferno_arch(){
	# Function to get the architecture for Inferno OS binaries.
	realarch="$1"
	if [ "$realarch" = "x86_64" ] ; then
		echo "386"
	else
		echo "$realarch"
	fi
}
# Main varibles for my workflow. All the paths depends on this.
# Lazy to type uname every fucking time.
	expvar ARCH "`uname -m`"
	expvar OPSYS "`uname -s`"
	expvar KERNEL_RELEASE "`uname -r`"
	expvar PROFILE "$HOME/.profile"
	expvar ETC "$HOME/etc"
	expvar LIB "$HOME/lib"
	expvar GIT "$HOME/git"
	expvar SHARE "$HOME/share"
	expvar BIN "$HOME/bin"
	expvar CBIN "$HOME/cbin"
	expvar TMPS "$HOME/tmps"
	expvar TMP "$HOME/tmp"
	expvar MNT "$HOME/mnt"
	expvar DRV "$HOME/drv"
	expvar ATK "$TMP/atk"
	expvar SCRIPTS "$HOME/.scripts"
	expvar IMGS "$HOME/imgs"
	expvar AUDIO "$HOME/audio"
	expvar DOCS "$HOME/docs"
	expvar VIDEO "$HOME/video"
	expvar DOWNLOADS "$HOME/downloads"
# Home.
	# Not linked to anything, with temporary files exact
	# for every system.
expvar HOMEMKDIRS "$DRV $MNT $ATK $TMP $BIN $CBIN $GIT \
		$DOWNLOADS"
# Inferno.
	expvar INFERNO "$HOME/inferno"
	expvar INFERNOARCH "`get_inferno_arch $ARCH`"
	expvar INFERNOBIN "$INFERNO/$OPSYS/$INFERNOARCH/bin"
# Stream edit commands. In general are used for acme, edwood, and sam.
	expvar SEUTILS "$HOME/.se"
# Acme.
	expvar ACMEMNT "$MNT/acme"
# Go.
	expvar GOPATH "$HOME/go"
	expvar GOBIN "$GOPATH/bin"
# Man.
	expvar MANPATH "$SHARE/man:$MANPATH"
	MANSECT="1:n:l:8:3:0:2:5:4:9:6:7"
	# My man sections which I'll maybe use later.
	MANSECT="$MANSECT:m"
	expvar MANSECT "$MANSECT"
# pkgsrc.
	expvar PKG_VERSIONS "$KERNEL_RELEASE"
	PKG_PATH="$PKG_PATH;http://cdn.NetBSD.org/pub/pkgsrc/packages/$OPSYS/$ARCH/$KERNEL_RELEASE/All"
	expvar PKG_PATH "$PKG_PATH"
# General.
	expvar NATIVE_KEYBOARD_MAP "ru"
	expvar KEYBOARD_OPTION "grp:alt_space_toggle"
	expvar NATIVE_KEYBOARD_LAYOUT "-layout $NATIVE_KEYBOARD_MAP,us -variant ,dvorak -option $KEYBOARD_OPTION"
	expvar QWERTY_KEYBOARD_LAYOUT "-layout us,$NATIVE_KEYBOARD_MAP -option $KEYBOARD_OPTION"
	expvar DVP_KEYBOARD_LAYOUT "-layout us,$NATIVE_KEYBOARD_MAP -variant dvp, -option $KEYBOARD_OPTION"
	expvar DVORAK_KEYBOARD_LAYOUT "-layout us,$NATIVE_KEYBOARD_MAP -variant dvorak, -option $KEYBOARD_OPTION"
	expvar KEYBOARD_REPEAT_RATE  60
	expvar KEYBOARD_REPEAT_DELAY 300
# Xmodmap.
	expvar XMODMAP "$HOME/.Xmodmap"
	expvar XRESOURCES "$HOME/.Xresources"
# Path.
	# Inferno programs.
	expvar PATH "$PATH:$INFERNOBIN"
	# Make possible to use programs installed by go.
	expvar PATH "$GOBIN:$PATH"
	# Users programs.
	expvar PATH "$CBIN:$BIN:$PATH"
# Program preferences now.
# WM.
	expvar XWM "`progpref vvm  wm rio dwm i3 9wm awesome startxfce4 `"
	expvar WWM "`progpref sway weston`"
# XDG.
	expvar XDG_CONFIG_HOME "$HOME/.config"
# Locale.
	expvar LANG "C"
	expvar LANGUAGE "C"
	expvar LC_ALL "C"
	expvar LC_CTYPE "ru_RU.UTF-8"
# DWM/JDWM.
	expvar XSTATUS "`progpref jstatus slstatus`"
# Wayland.
	expvar WLC_REPEAT_RATE "$KEYBOARD_REPEAT_RATE"
	expvar WLC_REPEAT_DELAY "$KEYBOARD_REPEAT_DELAY"
# Path getting executables.
	expvar XPATH "`progpref dmenu_path `"
# Menu.
	expvar WMENU "`progpref dmenu-wl`"
	expvar XMENU "`progpref dmenu`"
	expvar TMENU "`progpref fzf`"
	expvar XFALLMENU "`progpref 9menu`"
	expvar WFALLMENU "`progpref 9menu`"
# Lock.
	expvar XLOCK "`progpref jlock slock`"
	expvar WLOCK "`progpref swaylock`"
# Tabs manager.
	expvar XTABS_MANAGER "`progpref jtabbed tabbed`"
# Shell.
	expvar ENV "$HOME/.shrc"
	expvar RUNMENUPROMPT "& "
	expvar TEMENUPROMPT "! "
	expvar ITEMENUPROMPT "i! "
	expvar RCPROMPT "; "
	expvar SHPROMPT "! "
	expvar EDPROMPT "* "
	expvar R2PROMPT "@ "
	expvar VIPROMPT ": "
	expvar TCLPROMPT "% "
	expvar SHELL "`progpref rc dash nash ash sh`"
	expvar ISHELL "`progpref zsh csh ksh bash sh`"
# Editor.
	expvar EDITOR "`progpref ed ex cat`"
	expvar IEDITOR "`progpref nvim vim vi nano pico cat`"
	expvar GIT_EDITOR "$EDITOR"
	expvar IGIT_EDITOR "$IEDITOR"
	expvar VISUAL "`progpref edwood acme sam gvim notepadqq leafpad mousepad adie gedi xedit`" # Terminal.
	expvar WTERMINAL "`progpref termite`"
	expvar XTERMINAL "`progpref cli_dumb 9term jst st termite urxvt uxterm`"
	expvar IXTERMINAL "`progpref jst st termite urxvt uxterm`"
	expvar IWTERMINAL "`progpref termite`"
	expvar TERMINAL_SESSION_MANAGER "`progpref tmux screen`"
# Pager.
	expvar PAGER "`progpref nobs more most less cat`"
	expvar IPAGER "`progpref most less more cat`"
	expvar LESS "-R"
	expvar MANPAGER "$PAGER"
	expvar IMANPAGER "$IPAGER"
	expvar HELPPAGER "$PAGER"
	expvar IHELPPAGER "$IMANPAGER"
# Less colors.
	expvar LESS_TERMCAP_md  `perl -e 'print "\033[1;31m" ;'`
	expvar LESS_TERMCAP_me  `perl -e 'print "\033[0m"    ;'`
	# Underlined.
	expvar LESS_TERMCAP_us  `perl -e 'print "\033[1;32m" ;'`
	expvar LESS_TERMCAP_ue  `perl -e 'print "\033[0m"    ;'`
	# Service info.
	expvar LESS_TERMCAP_so  `perl -e 'print "\033[1;33m" ;'`
	expvar LESS_TERMCAP_se  `perl -e 'print "\033[0m"    ;'`
	# Blinking color.
	expvar LESS_TERMCAP_mb  `perl -e 'print "\033[1;34m" ;'`
	expvar LESS_TERMCAP_me  `perl -e 'print "\033[0m"    ;'`
# Browser.
	expvar XBROWSER "`progpref surf waterfox firefox chromium opera midori epiphany  luakit vimb`"
	expvar WBROWSER "`progpref waterfox firefox midori epiphany`"
	expvar ITBROWSER "`progpref w3m links2 links lynx elinks`"
	expvar BROWSER  "$TBROWSER"
# Documents.
	expvar DOCVIEWER "`progpref mupdf`"
# Images.
	expvar IMGVIEWER "`progpref feh`"
# Video.
	expvar VIDEOPLAYER "`progpref mpv ffplay`"
# Sudo.
	expvar SUDO_PROMPT "#"
# Sound.
	expvar JACKSNDDRIVE "alsa"
	expvar JACKDPSRATE "48000"
	expvar JACKSNDDEVICE "hw:0"
	expvar JACKNPERIODS "2"
	expvar JACKDPSPERIOD "64"
	expvar PACHANNELSNUM "2"
# GCC.
	expvar C_INCLUDE_PATH "$S/modules/c:$C_INCLUDE_PATH"
# Perl.
	expvar PERL5LIB "$S/modules/pl/5:$PERL5LIB"
# Python.
	expvar PYTHONPATH "$CODE/modules/py:$PYTHONPATH"
# Nethack.
	expvar NETHACKOPTIONS "boulder:0, color, autodig"
# Irrlicht.
	expvar IRRLICHT "$HOME/irrlicht"
# NTK.
	expvar PATHTO_NTK "$GIT/ntk"
# zsh.
	expvar ZSH_VERBOSE_MODE "false"
